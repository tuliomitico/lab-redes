	; End of File
	
	}
    }
    custom-config {
	custom-config-id service:UserDefined:/etc/bind/named.conf
	custom-command /etc/bind/named.conf
	config {
	options {
	  allow-query { any; };
	  allow-query-cache {
	    127.0.0.0/8;
	    192.0.2.0/24;
	  };
	  allow-recursion {
	    127.0.0.0/8;
	    192.0.2.0/24;
	  };
	  disable-empty-zone "2.0.192.in-addr.arpa";
	  disable-empty-zone "100.51.198.in-addr.arpa";
	  disable-empty-zone "113.0.203.in-addr.arpa";
