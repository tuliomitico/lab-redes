	zone "." {
	  type hint;
	  file "/etc/bind/db.root";
	};
	
	}
    }
}

node n3 {
    type router
    model router
    network-config {
	hostname n3Router
	!
	interface eth1
	 ipv6 address 2001:db8:ca5a:2::1/64
	 ip address 192.0.2.129/25
	!
	interface eth0
	 ip address 198.51.100.2/25
	 ipv6 address 2001:db8:feed::2/64
	!
	interface eth2
	 ip address 192.0.2.1/25
	 ipv6 address 2001:db8:ca5a:1::1/64
	!
